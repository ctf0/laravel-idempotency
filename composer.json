{
    "name": "square1/laravel-idempotency",
    "description": "Add idempotency to Laravel-based APIs, preventing duplicate request processing.",
    "type": "library",
    "keywords": [
        "square1",
        "laravel-idempotency",
        "idempotency",
        "api"
    ],
    "license": "MIT",
    "homepage": "https://github.com/square1-io/laravel-idempotency",
    "autoload": {
        "psr-4": {
            "Square1\\LaravelIdempotency\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Square1\\LaravelIdempotency\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Paul Conroy",
            "email": "paul@square1.io"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },

    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Square1\\LaravelIdempotency\\IdempotencyServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^11.0",
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0",
        "laravel/pint": "^1.14"
    }
}
